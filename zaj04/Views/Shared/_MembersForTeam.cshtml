@model zaj04.Models.MembersForTeamViewModel

<h4>@Model.TeamName</h4>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Members[0].Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Members[0].MemberType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model.Members) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MemberType)
        </td>
    </tr>
}

</table>
